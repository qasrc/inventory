<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">

<configuration>

	<!-- 进行mybatis 配置 -->
	<typeAliases>
		<typeAlias type="com.msunsoft.utils.PageInfo" alias="PageInfo" />
		<typeAlias type="com.msunsoft.model.Sale_Order" alias="Sale_Order" />
	</typeAliases>

	<!-- 引入自定义 mapper 文件 -->
	<mappers>
		<mapper resource="config/mybatis/User_Bill_Mapper.xml" />
		<mapper resource="config/mybatis/Popedom_Bill_Mapper.xml" />
		<mapper resource="config/mybatis/Resources_Bill_Mapper.xml" />
		<mapper resource="config/mybatis/Resources_Popedom_Bill_Mapper.xml" />
		<mapper resource="config/mybatis/infoManager/Client_BillMapper.xml" />
		<mapper resource="config/mybatis/infoManager/Suppliers_BillMapper.xml" />
		<mapper resource="config/mybatis/infoManager/Operator_BillMapper.xml" />
		<mapper resource="config/mybatis/Sale/Sale_Ordermapper/Sale_Ordermapper.xml" />

	</mappers>
</configuration> 